package Day2;

public class NumberPattern {

	public static void main(String[] args) {
		// row
		int n=5;
		for(int i=1;i<=n;i++) {
	   //i=1;1<=5 true
		//i++ =1++ 2
		
		  for(int j=1;j<=i;j++) {
			  //j=1 1<=1 true1 j++ =1++ 2
			  //j=2 2<=1 false
			  System.out.print(i);
			  //1
			  //2
		  
		  }
		  System.out.println(" ");
	}

	}
	
}
